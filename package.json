{
	"name": "eslint-plugin-wikitext",
	"version": "0.0.1",
	"description": "ESLint plugin for Wikitext",
	"keywords": [
		"eslint",
		"eslintplugin",
		"eslint-plugin",
		"mediawiki",
		"wikitext"
	],
	"homepage": "https://github.com/bhsd-harry/eslint-plugin-wikitext#readme",
	"bugs": {
		"url": "https://github.com/bhsd-harry/eslint-plugin-wikitext/issues"
	},
	"license": "GPL-3.0",
	"author": "Bhsd",
	"files": [
		"/lib",
		"/docs"
	],
	"main": "./lib/index.js",
	"exports": "./lib/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bhsd-harry/eslint-plugin-wikitext.git"
	},
	"scripts": {
		"lint": "npm-run-all \"lint:*\"",
		"lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
		"lint:js": "eslint .",
		"test": "mocha tests --recursive",
		"update:eslint-docs": "eslint-doc-generator --config-emoji='inherited,🌐'"
	},
	"dependencies": {
		"requireindex": "^1.2.0",
		"eslint-parser-wikitext": "^0.0.2"
	},
	"peerDependencies": {
		"eslint": ">=7"
	},
	"devDependencies": {
		"eslint": "^8.36.0",
		"eslint-doc-generator": "^1.0.0",
		"eslint-plugin-eslint-plugin": "^5.0.0",
		"eslint-plugin-n": "^15.6.0",
		"eslint-plugin-json-es": "^1.5.7",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"mocha": "^10.0.0",
		"npm-run-all": "^4.1.5"
	},
	"engines": {
		"node": "^18.4.0"
	}
}
